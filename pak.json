{
    "name": "esp-angular-mgr-skeleton",
    "description": "ESP Management Console Skeleton",
    "version": "5.5.1",
    "warn": "This pak requires Angular 1 which is no longer supported.",
    "keywords": [
        "esp"
    ],
    "licenses": [
        {
            "type": "GPL",
            "url": "http://www.gnu.org/licenses/gpl-2.0.html"
        },
        {
            "type": "Commercial",
            "url": "https://embedthis.com/licensing/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/esp-angular-mgr-skeleton.git"
    },
    "dependencies": {
        "embedthis/esp-angular-best": "~5.5",
        "embedthis/esp-angular-gui": "~5.5"
    },
    "devDependencies": {
        "pak": ">=0.11.3"
    },
    "blend": {
        "devDependencies": {
            "?esp": ">=7.0"
        },
        "?profile": "debug",
        "?import": true
    },
    "export": {
        "from": [
            "contents/**",
            "layouts/**",
            "partials/**"
        ],
        "to": "${TOP}",
        "overwrite": false
    }
}
